<?xml version="1.0" encoding="UTF-8"?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="SessionTrack.HomeOverviewPage" Title="Home" Icon="homepage.png" BackgroundColor="Teal">
    

        <StackLayout HorizontalOptions="CenterAndExpand" VerticalOptions="CenterAndExpand" Padding="35,0">
            
            <Label Text="Overview" TextColor="White" FontSize="36" HorizontalTextAlignment="Center"></Label>
            <Label Text="You have completed 25/60 minutes of practice today." TextColor="White" FontSize="16" HorizontalTextAlignment="Center"></Label>
            <BoxView HeightRequest="1" Color="White"></BoxView>
        
            <Button Text="Record Session" BackgroundColor="White" TextColor="Black" BorderRadius="10" Margin="0,2"></Button>
            <Button Text="Set Goals" BackgroundColor="White" TextColor="Black" BorderRadius="10" Margin="0,2"></Button>
            <BoxView HeightRequest="1" Color="White"></BoxView>
            
            <Button Text="Find a Tutor" BackgroundColor="White" TextColor="Black" BorderRadius="10" Margin="0,2"></Button>
            <Button Text="Be a Tutor" BackgroundColor="White" TextColor="Black" BorderRadius="10" Margin="0,2"></Button>
            
        </StackLayout>
        

</ContentPage>
